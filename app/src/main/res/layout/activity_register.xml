<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/rfkwaompsga"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#E6E6E6"
        android:clipToPadding="false"
        android:paddingTop="89dp"
        android:paddingBottom="61dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/rhcg9z0aqkah"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="150dp"
                android:layout_marginBottom="16dp"
                android:text="Register"
                android:textColor="#FF9A01"
                android:textSize="34sp" />

            <TextView
                android:id="@+id/r2zarucd9fp4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="140dp"
                android:layout_marginBottom="18dp"
                android:text="Create your new account"
                android:textColor="#131313"
                android:textSize="14sp" />

            <LinearLayout
                android:id="@+id/name_layout"
                android:layout_width="326dp"
                android:layout_height="60dp"
                android:layout_marginHorizontal="43dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:elevation="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="10dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/name_icon"
                    android:layout_width="18dp"
                    android:layout_height="19dp"
                    android:layout_marginEnd="20dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />

                <EditText
                    android:id="@+id/name_txt"
                    android:layout_width="270dp"
                    android:layout_height="60dp"
                    android:layout_weight="1"
                    android:hint="Full Name"
                    android:textColorHint="#FF9A01"
                    android:textSize="20dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/email_layout"
                android:layout_width="326dp"
                android:layout_height="60dp"
                android:layout_marginHorizontal="43dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:elevation="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingHorizontal="10dp"
                android:paddingVertical="15dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/email_icon"
                    android:layout_width="23dp"
                    android:layout_height="14dp"
                    android:layout_marginEnd="17dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />

                <EditText
                    android:id="@+id/email_txt"
                    android:layout_width="259dp"
                    android:layout_height="60dp"
                    android:layout_weight="1"
                    android:hint="Email"
                    android:inputType="textEmailAddress"
                    android:textColorHint="#FF9A01"
                    android:textSize="20dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/password_layout"
                android:layout_width="326dp"
                android:layout_height="60dp"
                android:layout_marginHorizontal="43dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:elevation="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingHorizontal="10dp"
                android:paddingVertical="14dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/password_icon"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />

                <EditText
                    android:id="@+id/password_txt"
                    android:layout_width="266dp"
                    android:layout_height="60dp"
                    android:layout_weight="1"
                    android:hint="    Password"
                    android:inputType="textPassword"
                    android:orientation="vertical"
                    android:textColorHint="#FF9A01"
                    android:textSize="20dp" />

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/password_visibility"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/weight_layout"
                android:layout_width="326dp"
                android:layout_height="60dp"
                android:layout_marginHorizontal="43dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:elevation="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="10dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/weight_icon"
                    android:layout_width="18dp"
                    android:layout_height="19dp"
                    android:layout_marginEnd="20dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />

                <EditText
                    android:id="@+id/weight_txt"
                    android:layout_width="270dp"
                    android:layout_height="60dp"
                    android:layout_weight="1"
                    android:hint="Weight(KG)"
                    android:inputType="numberDecimal"
                    android:textColorHint="#FF9A01"
                    android:textSize="20dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/height_layout"
                android:layout_width="326dp"
                android:layout_height="60dp"
                android:layout_marginHorizontal="43dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:elevation="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:padding="10dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/height_icon"
                    android:layout_width="18dp"
                    android:layout_height="19dp"
                    android:layout_marginEnd="20dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />

                <EditText
                    android:id="@+id/height_txt"
                    android:layout_width="270dp"
                    android:layout_height="60dp"
                    android:layout_weight="1"
                    android:hint="Height(CM)"
                    android:inputType="numberDecimal"
                    android:textColorHint="#FF9A01"
                    android:textSize="20dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rde99tchk4zu"
                android:layout_width="326dp"
                android:layout_height="60dp"
                android:layout_marginHorizontal="42dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:elevation="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingHorizontal="10dp"
                android:paddingVertical="15dp">

                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/question_icon"
                    android:layout_width="18dp"
                    android:layout_height="19dp"
                    android:layout_marginEnd="19dp"
                    android:scaleType="fitXY"
                    android:src="@mipmap/ic_launcher" />

                <Spinner
                    android:id="@+id/spinner"
                    android:layout_width="279dp"
                    android:layout_height="71dp"
                    android:layout_weight="1"
                    android:entries="@array/spinner_items"
                    android:spinnerMode="dropdown"
                />
            </LinearLayout>


            <Button
                android:id="@+id/login_button"
                android:layout_width="261dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="80dp"
                android:background="@drawable/cr20bff9a01cf"
                android:text="Login"
                android:textColor="#FFFFFF"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/singIn_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="140dp"
                android:text="Already have an account? Sign in "
                android:textColor="#FF9A01"
                android:textSize="10sp" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>