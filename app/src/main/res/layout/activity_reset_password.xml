<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:id="@+id/main"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:clipToPadding="false"
        android:id="@+id/rnzp0rrhsbyj"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#E6E6E6"
        android:paddingTop="100dp"
        android:paddingBottom="254dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <TextView
                android:id="@+id/rj79xpfhlhoc"
                android:textColor="#FF9A01"
                android:textSize="32sp"
                android:layout_marginBottom="46dp"
                android:layout_marginStart="49dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Reset Password"
                />
            <TextView
                android:id="@+id/rmy4bk3kc41h"
                android:textColor="#626263"
                android:textSize="13sp"
                android:layout_marginBottom="7dp"
                android:layout_marginStart="58dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Enter your email"
                />
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/r1othjbxlz9j"
                android:layout_width="274dp"
                android:layout_height="44dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:paddingHorizontal="10dp"
                android:layout_marginBottom="29dp"
                android:layout_marginHorizontal="35dp"
                android:elevation="10dp"
                android:orientation="vertical">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/rc89zbvy1jx"
                    android:layout_width="23dp"
                    android:layout_height="14dp"
                    android:layout_marginTop="15dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/r0qkmpb6q88tk"
                android:textColor="#626263"
                android:textSize="13sp"
                android:layout_marginBottom="4dp"
                android:layout_marginStart="57dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="Security Question"
                />
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/rx3yny77ue4"
                android:layout_width="274dp"
                android:layout_height="44dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:paddingHorizontal="12dp"
                android:layout_marginBottom="24dp"
                android:layout_marginHorizontal="38dp"
                android:elevation="10dp"
                android:orientation="vertical">
                <com.google.android.material.imageview.ShapeableImageView
                    android:id="@+id/r5gyinnlqheu"
                    android:layout_width="18dp"
                    android:layout_height="19dp"
                    android:layout_marginTop="11dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/rynhxrrmbc4m"
                android:layout_width="274dp"
                android:layout_height="44dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:layout_marginBottom="25dp"
                android:layout_marginHorizontal="32dp"
                android:elevation="10dp"
                android:orientation="vertical">
            </LinearLayout>
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/r6b10fu8j2pj"
                android:layout_width="271dp"
                android:layout_height="44dp"
                android:background="@drawable/s00000040sw1cr12bc4c4c4"
                android:layout_marginBottom="33dp"
                android:layout_marginHorizontal="32dp"
                android:elevation="10dp"
                android:orientation="vertical">
            </LinearLayout>
            <LinearLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/rzaki7d11iyo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/cr20bff9a01cf"
                android:paddingVertical="17dp"
                android:layout_marginHorizontal="116dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r3rmuf5mla1u"
                    android:textColor="#FFFFFF"
                    android:textSize="13sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Reset"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>